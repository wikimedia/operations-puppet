class baseaccount {
	$enabled = "true"

        if !defined(Class["nfs::home"]) {
                $manage_home = "true"
        }	

	Ssh_authorized_key {
		ensure => $enabled ? {
			"false" => 'absent',
			 default => 'present',
			}
	}
}

