[access "refs/*"]
	owner = group ops
	read = group Anonymous Users
	create = group ops
	submit = group ops
[project]
	description = Public puppet repository for Wikimedia Foundation operations team.
	state = active
[access "refs/heads/testlabs/*"]
	owner = group testlabs
	create = group testlabs
	push = group testlabs
	submit = group testlabs
	pushMerge = group testlabs
	pushTag = group testlabs
[receive]
	requireChangeId = true
[submit]
	mergeContent = true
[access "refs/for/refs/heads/test"]
	push = deny group Registered Users
	pushMerge = deny group Registered Users
	exclusiveGroupPermissions = push pushMerge
[access "refs/for/refs/heads/testlabs/*"]
	create = group testlabs
	push = group testlabs
	pushMerge = group testlabs
	pushTag = group testlabs
[access "refs/for/refs/heads/*"]
	push = group ops
	pushMerge = group ops
	pushTag = group ops
