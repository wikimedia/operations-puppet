[Unit]
Description=Precached daemon
After=network-online.target

[Service]
User=www-data
Group=www-data
WorkingDirectory=<%= @working_dir %>
ExecStart=<%= @venv_path %>/bin/ores precached stream.wikimedia.org https://ores.wmflabs.org --config /srv/ores/config/config/

[Install]
WantedBy=multi-user.target
