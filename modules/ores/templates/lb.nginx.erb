# THIS FILE IS MANAGED BY PUPPET
upstream oresweb {
    <% @realservers.each do |realserver| %>
    server <%= realserver %>;
    <% end %>
}

proxy_cache_path /srv/nginx/cache/ keys_zone=one:10m;
proxy_temp_path /srv/nginx/tmp/;
server {
    listen 80;

    location / {
        proxy_cache one;
        proxy_pass oresweb;
    }
}
