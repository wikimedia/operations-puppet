# THIS FILE IS MANAGED BY PUPPET
upstream oresweb {
    <% @realservers.each do |realserver| %>
    server <%= realserver %>;
    <% end %>
}

proxy_cache_path /srv/nginx/cache/ keys_zone=ores:64m;
proxy_cache_valid 7d;

proxy_temp_path /srv/nginx/tmp/;
server {
    listen 80;

    location / {
        proxy_cache ores;
        proxy_pass oresweb;
    }
}
