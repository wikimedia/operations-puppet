[Unit]
Description=Sentry error logging and aggregation service
Documentation=http://sentry.readthedocs.org/en/latest/
After=network.target

[Service]
Type=simple
ExecStart=<%= @sentry_cli %> start
User=<%= @user %>
Group=<%= @group %>
Restart=always
RestartSec=3

[Install]
WantedBy=multi-user.target

