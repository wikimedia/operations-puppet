[Unit]
Description=Instance of Arc Lamp for <%= @description %> trace logs

[Service]
User=xenon
Group=xenon
Restart=always
ExecStart=/srv/deployment/performance/arc-lamp/arclamp-log.py /etc/arclamp-log-<%= @title %>.yaml

[Install]
WantedBy=multi-user.target
