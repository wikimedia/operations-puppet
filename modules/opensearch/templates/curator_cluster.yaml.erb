---
client:
  hosts:
<% @curator_hosts.each do |host| -%>
    - <%= host %>
<% end -%>
  port: <%= @http_port %>
  url_prefix:
  use_ssl: False
  timeout: 30
  master_only: False

logging:
  loglevel: DEBUG
  logfile:
  logformat: default
  blacklist: ['elasticsearch', 'urllib3']
