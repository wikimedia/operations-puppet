#####################################################################
### puppet:///modules/dumps/rsync/rsyncd.conf.slowparse_logs.erb
#####################################################################

# slow-parse logs contain long render times for Wiki articles per server.
# They are rsynced from MW log hosts to this directory.
[slow-parse]
uid = <%= @user %>
gid = <%= @group %>
read only = false
path = <%= @miscdatasetsdir %>/slow-parse
hosts allow = <%= @hosts_allow %>
