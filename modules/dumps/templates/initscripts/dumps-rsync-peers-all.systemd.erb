[Unit]
Description=Dumps rsyncer service
After=network.target

[Service]
User=root
ExecStart=/bin/bash /usr/local/bin/rsync-to-peers.sh --do_tarball --do_rsync_xml --do_rsync_misc --do_rsync_miscsubs --xmldumpsdir <%= @xmldumpsdir %>  --xmlremotedirs <%= @xmlremotedirs -%> --miscdumpsdir <%= @miscdumpsdir %>  --miscremotedirs <%= @miscremotedirs %> --miscsubdirs <%= @miscsubdirs %> --miscremotesubs <%= @miscremotesubs %>

SyslogIdentifier=%N

[Install]
WantedBy=multi-user.target
