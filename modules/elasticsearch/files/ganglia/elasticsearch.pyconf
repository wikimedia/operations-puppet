modules {
  module {
    name = "elasticsearch_monitoring"
    language = "python"

    param metric_group {
      value = "elasticsearch"
    }

    param host {
        value = "http://localhost:9200/"
    }
  }
}

collection_group {
  collect_every = 60
  time_threshold = 60

  metric {
    name_match = "es_(.+)"
  }
}

