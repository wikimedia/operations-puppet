[Unit]
Description=Kubelet

[Service]
ExecStart=/usr/local/bin/kubelet \
    --api-servers=http://<%= @master_ip %>:8080 \
    --config=/etc/kubernetes/manifests \
    --configure-cbr0=false \
    --cluster-dns=<%= @cluster_dns_ip %> \
    --cluster-domain=kube

[Install]
WantedBy=multi-user.target
