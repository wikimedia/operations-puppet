{"readonly": true, "resource": "swaggerapi"}
{"readonly": true, "nonResourcePath": "/api"}
<%- @users.each do |user| -%>
<%- if user['type'] == 'namespaced' %>
{"user": "<%= user['name'] %>", "namespace": "<%= user['name'] %>"}
<%- elsif user['type'] == 'infrastructure' or user['type'] == 'admin' -%>
{"user": "<%= user['name'] %>"}
<%- end -%>
<%- end -%>
