apiVersion: v1
kind: Config
preferences: {}
clusters:
  - cluster:
      server: https://<%= @master_host %>:6443
    name: default
contexts:
  - context:
      cluster: default
      user: <%= @username %>
      {% if @namespace %}namespace: <%= @namespace %>{% end -%}
    name: default
current-context: default
users:
  - name: <%= @username %>
    user:
      token: <%= @token %>
