apiVersion: v1
kind: Config
preferences: {}
clusters:
  - cluster:
      server: https://<%= @master_host %>:6443
      certificate-authority: /etc/ssl/certs/Puppet_Internal_CA.pem
    name: default
contexts:
  - context:
      cluster: default
      user: client-infrastructure
    name: default
current-context: default
users:
  - name: client-infrastructure
    user:
      token: <%= @client_token %>
