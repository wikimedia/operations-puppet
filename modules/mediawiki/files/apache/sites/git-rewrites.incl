    # rewrites for old git.wikimedia.org URLs
    RewriteCond %{HTTP_HOST} =git.wikimedia.org
    RewriteCond %{QUERY_STRING} ^(\w+)=(\w+)/(\w+).git$
    RewriteRule ^/summary https://phabricator.wikimedia.org/r/p/%2/%3? [R=301,NE]

    RewriteCond %{HTTP_HOST} =git.wmflabs.org
    RewriteCond %{QUERY_STRING} ^(\w+)=(\w+)/(\w+).git$
    RewriteRule ^/tree https://phabricator.wikimedia.org/r/p/%2/%3;browse/ [R=301,NE]

