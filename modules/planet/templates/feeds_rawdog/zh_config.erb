# rawdog config file for zh.planet

plugindirs plugins
splitstate true
maxarticles 200
maxage 0
expireage 7d
keepmin 20
currentonly false
daysections true

dayformat %A, %d %B
timesections true
timeformat %I:%M %p
datetimeformat %H:%M, %A, %d %B

template /etc/rawdog/theme/wikimedia/zh/rd_page.tmpl
itemtemplate /etc/rawdog/theme/wikimedia/zh/rd_item.tmpl

feeditemtemplate default

outputfile /var/www/planet/zh/index.html

userefresh true
showfeeds true

numthreads 1
timeout 30s

ignoretimeouts false
showtracebacks false
verbose false

blocklevelhtml true
tidyhtml true
sortbyfeeddate true
useids true

hideduplicates id
newfeedperiod 3h

changeconfig false

