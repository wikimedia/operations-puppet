# rawdog config file for it.planet

plugindirs plugins
splitstate true
maxarticles 200
maxage 0
expireage 7d
keepmin 20
currentonly false
daysections true

dayformat %Y-%m-%d
timesections true
timeformat %H:%M
datetimeformat %Y-%m-%d %H:%M

pagetemplate /etc/rawdog/theme/wikimedia/it/rd_page.tmpl
itemtemplate /etc/rawdog/theme/wikimedia/it/rd_item.tmpl
feedlisttemplate /etc/rawdog/theme/wikimedia/it/rd_feedlist.tmpl
feeditemtemplate /etc/rawdog/theme/wikimedia/it/rd_feeditem.tmpl

outputfile /var/www/planet/it/index.html

userefresh true
showfeeds true

numthreads 1
timeout 30s

ignoretimeouts false
showtracebacks false
verbose false

blocklevelhtml true
tidyhtml true
sortbyfeeddate true
useids true

hideduplicates id
newfeedperiod 3h

changeconfig false

