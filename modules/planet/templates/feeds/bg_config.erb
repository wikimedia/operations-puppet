# Global configuration

[Planet]

name            = bg.planet.wikimedia
link            = https://bg.planet.<%= scope.lookupvar('planet::planet_domain_name') %>/
owner_name      = planet
owner_email     = planet@wikimedia.org
output_theme    = /usr/share/planet-venus/theme/wikimedia/bg
cache_directory = /var/cache/planet/bg/
output_dir      = /var/www/planet/bg/
feed_timeout    = 20
items_per_page  = 60
log_level       = DEBUG

