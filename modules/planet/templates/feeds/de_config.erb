# Global configuration

[Planet]

name            = de.planet.wikimedia
link            = https://de.planet.<%= scope.lookupvar('planet::planet_domain_name') %>/
owner_name      = planet
owner_email     = planet@wikimedia.org
output_theme    = /usr/share/planet-venus/theme/wikimedia/de
cache_directory = /var/cache/planet/de/
output_dir      = /var/www/planet/de/
feed_timeout    = 20
items_per_page  = 60
log_level       = DEBUG

