# Global configuration

[Planet]

name            = cs.planet.wikimedia
link            = http://cs.planet.<%= @domain_name %>/
owner_name      = planet
owner_email     = planet@wikimedia.org
output_theme    = /usr/share/planet-venus/theme/wikimedia/cs
cache_directory = /var/cache/planet/cs/
output_dir      = /var/www/planet/cs/
feed_timeout    = 20
items_per_page  = 60
log_level       = DEBUG

