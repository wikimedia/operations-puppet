# Global configuration

[Planet]

name            = pl.planet.wikimedia
link            = https://pl.planet.<%= scope.lookupvar('planet::domain_name') %>/
owner_name      = planet
owner_email     = planet@wikimedia.org
output_theme    = /usr/share/planet-venus/theme/wikimedia/pl
cache_directory = /var/cache/planet/pl/
output_dir      = /var/www/planet/pl/
feed_timeout    = 20
items_per_page  = 60
log_level       = DEBUG

