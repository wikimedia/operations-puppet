# Global configuration

[Planet]

name            = pt.planet.wikimedia
link            = http://pt.planet.<%= scope.lookupvar('planet::planet_domain_name') %>/
owner_name      = planet
owner_email     = planet@wikimedia.org
output_theme    = /usr/share/planet-venus/theme/wikimedia/pt
cache_directory = /var/cache/planet/pt/
output_dir      = /var/www/planet/pt/
feed_timeout    = 20
items_per_page  = 60
log_level       = DEBUG

