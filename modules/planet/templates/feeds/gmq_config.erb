# Global configuration

[Planet]

name            = gmq.planet.wikimedia
link            = http://gmq.planet.<%= scope.lookupvar('planet::planet_domain_name') %>/
owner_name      = planet
owner_email     = planet@wikimedia.org
output_theme    = /usr/share/planet-venus/theme/wikimedia/gmq
cache_directory = /var/cache/planet/gmq/
output_dir      = /var/www/planet/gmq/
feed_timeout    = 20
items_per_page  = 60
log_level       = DEBUG

