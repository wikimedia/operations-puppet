# Global configuration

[Planet]

name            = ro.planet.wikimedia
link            = http://ro.planet.<%= scope.lookupvar('planet::planet_domain_name') %>/
owner_name      = planet
owner_email     = planet@wikimedia.org
output_theme    = /usr/share/planet-venus/theme/wikimedia/ro
cache_directory = /var/cache/planet/ro/
output_dir      = /var/www/planet/ro/
feed_timeout    = 20
items_per_page  = 60
log_level       = DEBUG

