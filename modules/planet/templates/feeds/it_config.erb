# Global configuration

[Planet]

name            = it.planet.wikimedia
link            = http://it.planet.<%= scope.lookupvar('planet::planet_domain_name') %>/
owner_name      = planet
owner_email     = planet@wikimedia.org
output_theme    = /usr/share/planet-venus/theme/wikimedia/it
cache_directory = /var/cache/planet/it/
output_dir      = /var/www/planet/it/
feed_timeout    = 20
items_per_page  = 60
log_level       = DEBUG

