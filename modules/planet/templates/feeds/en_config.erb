# Global configuration

[Planet]

name            = en.planet.wikimedia
link            = http://en.planet.<%= scope.lookupvar('planet::planet_domain_name') %>/
owner_name      = planet
owner_email     = planet@wikimedia.org
output_theme    = /usr/share/planet-venus/theme/wikimedia/en
cache_directory = /var/cache/planet/en/
output_dir      = /var/www/planet/en/
feed_timeout    = 20
items_per_page  = 60
log_level       = DEBUG

