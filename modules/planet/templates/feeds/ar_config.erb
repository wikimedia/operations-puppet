# Global configuration

[Planet]

name            = ar.planet.wikimedia
link            = http://ar.planet.<%= scope.lookupvar('misc::planet-venus::planet_domain_name') %>/
owner_name      = planet
owner_email     = planet@wikimedia.org
output_theme    = /usr/share/planet-venus/theme/wikimedia/ar
cache_directory = /var/cache/planet/ar/
output_dir      = /var/www/planet/ar/
feed_timeout    = 20
items_per_page  = 60
log_level       = DEBUG

