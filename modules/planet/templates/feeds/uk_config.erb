# Global configuration

[Planet]

name            = uk.planet.wikimedia
link            = http://uk.planet.<%= scope.lookupvar('planet::planet_domain_name') %>/
owner_name      = planet
owner_email     = planet@wikimedia.org
output_theme    = /usr/share/planet-venus/theme/wikimedia/uk
cache_directory = /var/cache/planet/uk/
output_dir      = /var/www/planet/uk/
feed_timeout    = 20
items_per_page  = 60
log_level       = DEBUG

