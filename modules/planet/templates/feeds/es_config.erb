# Global configuration

[Planet]

name            = es.planet.wikimedia
link            = http://es.planet.<%= scope.lookupvar('planet::planet_domain_name') %>/
owner_name      = planet
owner_email     = planet@wikimedia.org
output_theme    = /usr/share/planet-venus/theme/wikimedia/es
cache_directory = /var/cache/planet/es/
output_dir      = /var/www/planet/es/
feed_timeout    = 20
items_per_page  = 60
log_level       = DEBUG

