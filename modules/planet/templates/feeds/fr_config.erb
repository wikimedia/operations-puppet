# Global configuration

[Planet]

name            = fr.planet.wikimedia
link            = http://fr.planet.<%= scope.lookupvar('misc::planet-venus::planet_domain_name') %>/
owner_name      = planet
owner_email     = planet@wikimedia.org
output_theme    = /usr/share/planet-venus/theme/wikimedia/fr
cache_directory = /var/cache/planet/fr/
output_dir      = /var/www/planet/fr/
feed_timeout    = 20
items_per_page  = 60
log_level       = DEBUG

