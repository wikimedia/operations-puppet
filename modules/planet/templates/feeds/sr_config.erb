# Global configuration

[Planet]

name            = sr.planet.wikimedia
link            = http://sr.planet.<%= scope.lookupvar('planet::planet_domain_name') %>/
owner_name      = planet
owner_email     = planet@wikimedia.org
output_theme    = /usr/share/planet-venus/theme/wikimedia/sr
cache_directory = /var/cache/planet/sr/
output_dir      = /var/www/planet/sr/
feed_timeout    = 20
items_per_page  = 60
log_level       = DEBUG

