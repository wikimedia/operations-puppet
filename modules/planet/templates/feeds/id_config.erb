# Global configuration

[Planet]

name            = id.planet.wikimedia
link            = https://id.planet.<%= @domain_name %>/
owner_name      = planet
owner_email     = planet@wikimedia.org
output_theme    = /usr/share/planet-venus/theme/wikimedia/id
cache_directory = /var/cache/planet/id/
output_dir      = /var/www/planet/id/
feed_timeout    = 20
items_per_page  = 60
log_level       = DEBUG

