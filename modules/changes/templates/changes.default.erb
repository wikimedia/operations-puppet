# Config file for Changes
#
# This file is sourced by the changes/init Upstart task
# defined in </etc/init/changes/init.conf>.
#

# Address (as host:port) of redis server which publishes change events.
redis=<%= @redis %>

# Interface WebSocket server should bind to.
iface=<%= @iface %>

# Comma-separated list of ports on which to listen.
# An Changes instance will be spawned for each port.
ports=<%= @ports.join(',') %>
