[Unit]
Description=UDP log receiver (instance <%= @name %>)

[Service]
User=udp2log
ExecStart=/usr/bin/udp2log --port <%= @port %> --config-file /etc/udp2log/<%= @name %> --recv-queue <%= @recv_queue %>
EnvironmentFile=-/etc/default/udp2log-<%= @name %>
Restart=always

[Install]
WantedBy=multi-user.target
