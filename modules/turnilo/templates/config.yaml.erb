  # Netflow data
  - name: wmf_netflow
    title: wmf_netflow
    clusterName: druid-analytics-eqiad
    source: wmf_netflow
    description: Network data
    defaultSortMeasure: packets
    introspection: none
    options:
      customTransforms:
        direction:
          extractionFn:
            type: lookup
            retainMissingValue: true
            lookup:
              type: map
              map: {"1":"inbound", "2":"outbound","0":"unknown"}
        forwarded:
          extractionFn:
            type: lookup
            retainMissingValue: true
            lookup:
              type: map
              map: {"1":"yes", "0":"no"}
        tcp:
          extractionFn:
            type: lookup
            retainMissingValue: true
            lookup:
              type: map
              # see P13050 for how this list was generated
