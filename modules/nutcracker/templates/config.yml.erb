<%=
    require 'yaml'

    config = {}
    @pools.sort.each do |pool,settings|
        settings.sort.each do |k,v|
            settings[k] = case v.to_s
              when v.to_s.to_f.to_s then Float(v)
              when v.to_s.to_i.to_s then Integer(v)
              else v
            end
        end
        config[pool] = settings
    end
    config.to_yaml.gsub(/^---[^\n]*?\n/, '')
%>
