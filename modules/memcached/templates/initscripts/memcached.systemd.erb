[Unit]
Description=memcached daemon
After=network.target

[Service]
ExecStart=/usr/bin/memcached -p <%= scope['memcached::port'] -%> -u nobody -m <%= scope['memcached::size'] %> -c 25000 -l <%= scope['memcached::ip'] %> -n 5 -f <%= scope['memcached::growth_factor'] -%> <% scope['memcached::extra_options'].sort.each do |option,value| -%><%= option %> <%= value %> <% end -%>


[Install]
WantedBy=multi-user.target
