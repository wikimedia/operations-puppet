-- Grants for pc1, pc2, pc3, pc4, pc5
-- wikiadmin 10.%

GRANT PROCESS, REPLICATION CLIENT
    ON *.* TO '<%= @wikiadmin_username %>'@'10.%'
    IDENTIFIED BY '<%= @wikiadmin_pass %>';

GRANT ALL PRIVILEGES
    ON `parsercache`.* TO '<%= @wikiadmin_username %>'@'10.%';

-- wikiuser 10.%

GRANT PROCESS, REPLICATION CLIENT
    ON *.* TO '<%= @wikiuser_username %>'@'10.%'
    IDENTIFIED BY '<%= @wikiuser_pass %>';

GRANT SELECT, INSERT, UPDATE, DELETE
    ON `parsercache`.* TO '<%= @wikiuser_username %>'@'10.%';
