[client]
user     = cumin2024
<% if @password -%>
password = <%= @password %>
<% end -%>

<% if @labsdb_password -%>
[clientlabsdb]
user     = root
password = <%= @labsdb_password %>
<% end -%>

<% if @replication_password -%>
[clientreplication]
user     = <%= @replication_user %>
password = <%= @replication_password %>
<% end -%>

[mysql]
<% if @prompt -%>
prompt = '\u@<%= @prompt %>[\d]> '
<% else -%>
prompt = '\u@\h[\d]> '
<% end -%>
pager  = "grcat /etc/mysql/grcat.config | less -RSFXin"
