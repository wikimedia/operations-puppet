<%#- SPDX-License-Identifier: Apache-2.0 -%>
[Unit]
Description=Benthos socket for HAProxy

[Socket]
ListenDatagram=<%= @benthos_socket %>
Service=benthos@haproxy_cache_systemd_socket.service
SocketUser=benthos
<%- if @benthos_udp_rbuffer_bytes -%>
ReceiveBuffer=<%= @benthos_udp_rbuffer_bytes %>
<%- end -%>
[Install]
WantedBy=sockets.target

