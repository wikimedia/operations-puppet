<%- @abuse_networks.each_pair do |net_name, config| -%>
<%- if config.has_key?('comment') -%>
  <%- config['comment'].split("\n").each do |line| -%>
// <%= line %>
  <%- end -%>
<%- end -%>
acl <%= net_name %> {
  <%- config['networks'].each do |net| -%>
	"<%= net %>";
  <%- end -%>
}
<%- end -%>
