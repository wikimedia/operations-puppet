AUTO_DEBSIGN=${AUTO_DEBSIGN:-no}
if [ -z "${DIST}" ]; then
	DIST="jessie"
fi

DISTRIBUTION="$DIST"
PDEBUILD_PBUILDER="cowbuilder"
BASEPATH="/var/cache/pbuilder/base-$DIST.cow/"
HOOKDIR=/var/cache/pbuilder/hooks/$DIST
BUILDRESULT="/var/cache/pbuilder/result/$DIST"
APTCACHE="/var/cache/pbuilder/aptcache/$DIST"
