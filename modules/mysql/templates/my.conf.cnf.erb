# Note:  This file is managed by Puppet.

<% @settings.sort.each do |section, config|      -%>
[<%= section %>]
<%   content.sort.each do |key, values|          -%>
<%     [values].flatten.sort.each do |value|     -%>
<%= value == false ? '#' : '' %><%= key -%><%=
         case value
         when true, false
           ''
         else
           " = #{value}"
         end
%>
<%     end                                       -%>
<%   end                                         -%>

<% end                                           -%>
