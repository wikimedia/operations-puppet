repo_locations: {<% deployment_repo_locations.each do |repo, location| %>"<%= repo %>": "<%= location %>",<% end %>}
repo_urls: {<% deployment_repo_urls.each do |site, repos| %>"<%= site %>": [<% repos.each do |repo, url| %>"<%= repo %>": "<%= url %>"},<% end %>],<% end %>}
repo_regex: {<% deployment_repo_regex.each do |repo, regexes| %>"<%= repo %>": [<% regexes.each do |before, after| %>{"<%= before %>": "<%= after %>"},<% end %>],<% end %>}
