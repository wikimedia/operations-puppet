<% if @systemd_conf_format -%>
<% if @sid %><% @args += '--sid ' + @sid + ' ' -%><% end -%>
<% @args += @format + ' ' -%>
<% @args += @input + ' ' -%>
<% @args += @outputs.join("\n")  + ' ' -%>
<% if @output_invalid -%>
<% if @output_invalid == true or @output_invalid == 'true' -%>
<% @args += '--output-invalid ' -%>
<% else -%>
<% @args += '--output-invalid ' + @output_invalid + ' ' -%>
<% end -%>
ARGS=<%= @args -%>
<% else %>
<% if @sid -%>
--sid
<%= @sid %>
<% end -%>
<%= @format %>
<%= @input %>
<%= @outputs.join("\n") %>
<% if @output_invalid -%>
<% if @output_invalid == true or @output_invalid == 'true' -%>
--output-invalid
<% else -%>
--output-invalid <%= @output_invalid %>
<% end -%>
<% end -%>
<% end -%>