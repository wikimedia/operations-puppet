# This file is managed by puppet!
[Unit]
Description = <%= @title %> uwsgi app

[Service]
ExecStart = /usr/bin/uwsgi \
    --die-on-term \
    --autoload \
    <% if @plugins -%> --plugins '<%= @plugins %>' <% end -%> \
    --ini <%= @inipath %>
User = www-data
Group = www-data
SyslogIdentifier = uwsgi-<%= @basename %>

[Install]
WantedBy = multi-user.target
