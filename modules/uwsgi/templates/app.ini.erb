# uWSGI configuration for <%= @name %>
# This file is managed by Puppet.
<%- @settings.sort.each do |section, items| -%>
[<%= section %>]
    <%- items.sort.each do |key, value| -%>
        <%- if value.is_a?(Array) then -%>
            <%- value.each do |value_item| -%>
                <%= key %>=<%= value_item %>
            <%- end -%>
        <%- else -%>
            <%= key %>=<%= value %>
        <%- end -%>
    <%- end -%>
<%- end -%>
