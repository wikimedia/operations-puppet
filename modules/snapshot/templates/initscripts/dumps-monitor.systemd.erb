[Unit]
Description=Dumps monitoring service
After=network.target

[Service]
User=<%= @xmldumpsuser %>
WorkingDirectory=<%= @repodir %>
ExecStart=/bin/bash <%= @repodir -%>/monitor --configfile <%= @confsdir -%>/wikidump.conf.dumps:monitor --basedir <%= @repodir %>

[Install]
WantedBy=multi-user.target
