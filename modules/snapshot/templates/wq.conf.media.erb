[wiki]
mediawiki=<%= @apachedir %>
allwikislist=<%= @apachedir -%>/dblists/all.dblist
privatewikislist=<%= @apachedir -%>/dblists/private.dblist
closedwikislist=<%= @apachedir -%>/dblists/closed.dblist
adminsettings=private/PrivateSettings.php
skipwikislist=<%= scope.lookupvar('snapshot::dumps::dirs::wikiqueriesdir') -%>/dblists/skip.dblist

[output]
wikiqueriesdir=<%= scope.lookupvar('snapshot::dumps::dirs::datadir') -%>/public/other/imageinfo
temp=<%= scope.lookupvar('snapshot::dumps::dirs::datadir') -%>/temp
fileperms=0644

[database]

[tools]
php=/usr/bin/php5
mysql=/usr/bin/mysql
gzip=/bin/gzip
bzip2=/bin/bzip2
multiversion=<%= @apachedir -%>/multiversion/MWScript.php
