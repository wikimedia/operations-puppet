[database]
host = "<%= @database_host %>"
username = "<%= @database_username %>"
password = "<%= @database_password %>"
dbname = "<%= @database_name %>"
tables_prefix = "<%= @database_table_prefix %>"
; Enable LOAD FILE fix
; https://github.com/piwik/piwik/issues/9419#issuecomment-293663840
; https://piwik.org/faq/troubleshooting/#faq_194
adapter = MYSQLI

[General]
<%- @proxy_client_headers.each do |proxy_client_header| -%>
proxy_client_headers[] = "<%= proxy_client_header %>"
<%- end -%>
salt = "<%= @password_salt %>"
<%- @trusted_hosts.each do |trusted_host| -%>
trusted_hosts[] = "<%= trusted_host %>"
<%- end -%>
; Disable the Matomo UI showing a maintenance message
; This does not disable database updates, please also check 'record_statistics'
maintenance_mode = 0
; disable browser trigger archiving for all requests (even those with a segment)
; https://piwik.org/docs/setup-auto-archiving/#more-information-about-piwik-archiving
browser_archiving_disabled_enforce = 1
; https://matomo.org/faq/troubleshooting/faq_16646/
enable_internet_features = 0
enable_marketplace = 0
enable_tracking_failures_notification = 0


[Tracker]
;https://github.com/piwik/piwik/issues/6398#issuecomment-91093146
bulk_requests_use_transaction = 0
; If set to zero, it forces Matomo to quickly return the 1*1 gif beacon
; without connecting to the Database or doing any processing.
record_statistics = 1

[mail]
username = "<%= @admin_username %>"
password = "<%= @admin_password %>"

