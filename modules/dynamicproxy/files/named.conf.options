acl labs {
        10.0.0.0/8;
        localhost;
        localnets;
};

options {
        directory "/var/cache/bind";

        recursion yes;
        allow-query { labs; };
        forwarders { 10.68.16.1; };
        forward only;

        dnssec-enable yes;
        dnssec-validation yes;

        auth-nxdomain no;
};

