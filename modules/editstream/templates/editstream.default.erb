# Config file for EditStream
#
# This file is sourced by the editstream/init Upstart task
# defined in </etc/init/editstream/init.conf>.
#

# Address (as host:port) of redis server which publishes edit events.
redis=<%= @redis %>

# Interface WebSocket server should bind to.
iface=<%= @iface %>

# Comma-separated list of ports on which to listen.
# An EditStream instance will be spawned for each port.
ports=<%= @ports.join(',') %>
