#####################################################################
### THIS FILE IS MANAGED BY PUPPET
### puppet:///modules/base/logrotate/puppet
#####################################################################

/var/log/puppet {
    daily
    compress 
    missingok
    notifempty
    rotate 7
}

/var/log/puppet.log {
    daily
    compress
    missingok
    notifempty
    rotate 7
}
