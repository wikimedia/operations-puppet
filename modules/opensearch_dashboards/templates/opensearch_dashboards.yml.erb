<%#- SPDX-License-Identifier: Apache-2.0 -%>
# opensearchDashboards.defaultAppId deprecated as of 1.0.0
opensearchDashboards.defaultAppId: <%= @default_app_id %>
# allow importing <64mb backups
savedObjects.maxImportPayloadBytes: 67108864
logging.quiet: <%= @logging_quiet %>
metrics.enabled: <%= @metrics_enabled %>
<%- unless @region_map_enabled.nil? or @region_map_enabled == :undef -%>
region_map.enabled: <%= @region_map_enabled %>
<%- end -%>
server.maxPayloadBytes: <%= @server_max_payload_bytes %>
<%- unless @tile_map_enabled.nil? or @tile_map_enabled == :undef -%>
tile_map.enabled: <%= @tile_map_enabled %>
<%- end -%>
<%- unless @timelion_enabled.nil? or @timelion_enabled == :undef -%>
vis_type_timeline.enabled: <%= @timelion_enabled %>
<%- end -%>
<%- unless @index.nil? or @index == :undef -%>
opensearchDashboards.index: <%= @index %>
<%- end -%>
<%- unless @vega_enabled.nil? or @vega_enabled == :undef -%>
vis_type_vega.enabled: <%= @vega_enabled %>
<%- end -%>
