# This file is managed by Puppet.
# puppet:///logrotate/logrotate.erb

<%= @file_pattern %> {
<% if @not_if_empty -%>
    notifempty
<% end -%>
<% if @max_age -%>
    maxage <%= @max_age %>
<% end -%>
<% if @rotate -%>
    rotate <%= @rotate %>
<% end -%>
<% if @date_ext -%>
    dateext
<% end -%>
<% if @compress -%>
    compress
<% end -%>
<% if @delay_compress -%>
    delaycompress
<% end -%>
<% if @missing_ok -%>
    missingok
<% end -%>
<% if @size -%>
    size <%= @size %>
<% end -%>
}
