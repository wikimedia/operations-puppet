# Configuraton file for txStatsD
# This file is managed by Puppet
<%=
    @settings.sort.map { |section,items|
            ["[#{section}]"].concat items.sort.map { |k,v|
                v = v.is_a?(Array) ? v.join(',') : (v == :undef ? '' : v)
                "#{k} = #{v}"
            }.push []
    }.join("\n")
%>
