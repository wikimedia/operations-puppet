[agent]

[linux_bridge]
<% bmap = [] -%>
<% @bridge_mappings.sort.each  do |name,bridge| -%>
<% bmap.push(name + ':' + bridge) %>
<% end -%>
<% bmap_string = bmap.join(',') -%>
bridge_mappings = <%= bmap_string -%>

[vxlan]
enable_vxlan = True
# local_ip = OVERLAY_INTERFACE_IP_ADDRESS
local_ip = <%= @ipaddress %>
l2_population = True
