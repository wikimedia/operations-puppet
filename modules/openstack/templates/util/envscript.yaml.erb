# These settings are duplicates of the '<%= @title %>' section
#  in <%= @clouds_files %>. They're left here for easy discovery
#  and for tools that need access to the raw values but don't
#  want to parse clouds.yaml

OS_USERNAME: <%= @os_user %>
<% if defined?(@os_user_domain_id) -%>
OS_USER_DOMAIN_ID: <%= @os_user_domain_id %>
<% else -%>
OS_USER_DOMAIN_ID: UNSET
<% end -%>
OS_PASSWORD: "<%= @os_password %>"
OS_AUTH_URL: "https://<%= @keystone_api_fqdn %>:<%= @keystone_api_port %>/v3"
OS_REGION_NAME: "<%= @region %>"
OS_NO_CACHE: 1
OS_IDENTITY_API_VERSION: 3
OS_INTERFACE: <%= @keystone_api_interface %>
PYTHONIOENCODING: utf-8
<% if defined?(@os_project) -%>
OS_PROJECT_NAME: "<%= @os_project %>"
OS_PROJECT_ID: "<%= @os_project %>"
<% else -%>
OS_PROJECT_NAME: UNSET
OS_PROJECT_ID: UNSET
<% end -%>
<% if defined?(@os_project_domain_id) -%>
OS_PROJECT_DOMAIN_ID: <%= @os_project_domain_id %>
OS_DOMAIN_ID: <%= @os_project_domain_id %>
<% else -%>
OS_PROJECT_DOMAIN_ID: UNSET
OS_DOMAIN_ID: UNSET
<% end -%>
<% if defined?(@os_db_password) && ! @os_db_password.empty? %>
NOVA_MYSQL_PASS: "<%= @os_db_password %>"
<% end %>
OS_VOLUME_API_VERSION: 3.44
