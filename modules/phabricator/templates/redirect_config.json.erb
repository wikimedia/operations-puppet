{
    "query": "select t.id from maniphest_customfieldstorage f, maniphest_task t where t.PHID=f.objectPHID and f.fieldIndex='$fieldIndex' and f.fieldValue='$fieldValue'",
    "mysql": {
        "user": "<%= @mysql_user %>",
        "pass": "<%= @mysql_pass %>",
        "host": "<%= @mysql_host %>"
    },
    "fieldIndex": "<%= @field_index %>",
    "urlPatterns": [
        {
            "pattern": "(bugs|bugzila).wikimedia.org/show_bug.cgi\\?id=([0-9]+)",
            "fieldValue": "bz$2",
            "redirectUrl": "https://<%= @phab_host %>/T$id"
        },
        {
            "pattern": "(bugs|bugzila).wikimedia.org/([0-9]+)",
            "fieldValue": "bz$2",
            "redirectUrl": "https://<%= @phab_host %>/T$id"
        },
        {
            "pattern": "https://(bugs|bugzila).wikimedia.org/[a-zA-Z]+.cgi",
            "redirectUrl": "https://<%= @phab_host %>/"
        },
        {
            "pattern": "https://(bugs|bugzila).wikimedia.org/query.cgi",
            "redirectUrl": "https://<%= @phab_host %>/maniphest/query/advanced/"
        },
        {
            "pattern": "https://(bugs|bugzila).wikimedia.org/describecomponents.cgi",
            "redirectUrl": "https://<%= @phab_host %>/fact/"
        }
    ]
}
