{
  "query": "select t.id from maniphest_customfieldstorage f, maniphest_task t where t.PHID=f.objectPHID and f.fieldIndex=? and f.fieldValue=?",
  "mysql": {
    "user": "<%= @mysql_user %>",
    "pass": "<%= @mysql_pass %>",
    "host": "<%= @mysql_host %>"
  },
  "fieldIndex": "<%= @field_index %>",
  "urlPatterns": [
    {
      "pattern": "(bugs|bugzilla).wikimedia.org/show_bug.cgi\\?id=([0-9]+)",
      "fieldValue": "bz$2",
      "redirectUrl": "https://<%= @phab_host %>/T$id"
    },
    {
      "pattern": "(bugs|bugzilla).wikimedia.org/([0-9]+)",
      "fieldValue": "bz$2",
      "redirectUrl": "https://<%= @phab_host %>/T$id"
    },
    {
      "pattern": "(bugs|bugzilla).wikimedia.org/buglist.cgi\\?(.*)",
      "redirectUrl": "https://old-bugzilla.wikimedia.org/buglist.cgi?$2"
    },
    {
      "pattern": "(bugs|bugzilla).wikimedia.org/attachment.cgi\\?(.*)",
      "redirectUrl": "https://old-bugzilla.wikimedia.org/attachment.cgi?$2"
    },
    {
      "pattern": "(bugs|bugzilla).wikimedia.org/[a-zA-Z_]+.cgi(\\?.*)$",
      "redirectUrl": "https://<%= @phab_host %>/"
    },
    {
      "pattern": "(bugs|bugzilla).wikimedia.org/?",
      "redirectUrl": "https://<%= @phab_host %>/"
    }
  ]
}
