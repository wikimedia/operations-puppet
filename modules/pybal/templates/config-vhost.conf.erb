<VirtualHost *:80>
    ServerName pybal-config
    ServerAlias <%= @vhostnames.join(' ') %>
    
    DocumentRoot /srv/pybal-config

    <Directory "/srv/pybal-config">
	 <IfVersion >= 2.4>
         Require all granted
	</IfVersion>
	<IfVersion < 2.4>
         Order allow,deny
         Allow from all
	</IfVersion>
         Options +Indexes +Followsymlinks
         AllowOverride None
     </Directory>

     CustomLog ${APACHE_LOG_DIR}/pybal-access.log wmf
     ErrorLog ${APACHE_LOG_DIR}/pybal-error.log
</VirtualHost>
