# Config file for RCStream
#
# This file is sourced by the rcstream/init Upstart task
# defined in </etc/init/rcsteam/init.conf>.
#

# Address (as host:port) of redis server which publishes the events.
redis=<%= @redis %>

# Interface WebSocket server should bind to.
iface=<%= @iface %>

# Comma-separated list of ports on which to listen.
# An instance will be spawned for each port.
ports=<%= @ports.join(',') %>
