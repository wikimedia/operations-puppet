# This file is managed by Puppet.
# puppet:///statistics/discovery-stats.logrotate.erb
# Rotate statistics::discovery cron output logs
#
<%= @log_dir %>/*.log {
  notifempty
  maxage 10
  rotate 2
  dateext
  compress
  delaycompress
  missingok
  su <%= scope['statistics::discovery::user'] %> <%= scope['statistics::discovery::group'] %>
}
