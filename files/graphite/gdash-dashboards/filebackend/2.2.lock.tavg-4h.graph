title "LockManager and StreamFile Average Latency (ms) log(2) -8hours"
linewidth 1
logbase 2
hide_legend "false"
from "-8hours"
linemode "connected" 

field :lock, :alias => "none",
               :data  => 'cactiStyle(LockManager.*.tavg)'

field :stream, :alias => "none",
               :data  => 'cactiStyle(StreamFile.*.tavg)'
