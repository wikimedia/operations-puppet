title "LockManager and StreamFile Sampled Call Rate log(2) -1day"
linewidth 1
logbase 2
hide_legend "false"
from "-1day"
linemode "connected" 

field :lock, :alias => "none",
               :data  => 'cactiStyle(LockManager.*.count)'

field :stream, :alias => "none",
               :data  => 'cactiStyle(StreamFile.*.count)'
