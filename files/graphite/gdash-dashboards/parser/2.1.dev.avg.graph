title   "Slowest 14 Parser Methods by Max Average Time (ms) log(2) -1day"
linewidth 1
linemode "connected"
hide_legend "false"
logbase 2
hide_legend "false"
from "-1day"

field :devavg, :alias => "none",
               :data  => 'cactiStyle(substr(highestMax(maximumAbove(Parser.*.tavg,1),14),1,2))'
