title   "Parser Cache Hit Latency (ms) -1week"
vtitle "time in ms"
linewidth 2
linemode "connected"
hide_legend "false"
from "-1week"

field :hit, :color => "green",
               :alias => "none",
               :data  => 'cactiStyle(alias(MediaWiki.ParserCache.get.tavg,"Avg"))'

field :cant, :color => "red",
               :alias => "none",
               :data  => 'cactiStyle(alias(MediaWiki.ParserCache.get.tp99,"99%"))'
