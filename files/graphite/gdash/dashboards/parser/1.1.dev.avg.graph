title   "Slowest 10 Parser Methods by Max Average Time (ms) log(2) -8hours"
linewidth 1
linemode "connected"
hide_legend "false"
logbase 2
hide_legend "false"
from "-8hours"

field :devavg, :alias => "none",
               :data  => 'cactiStyle(substr(highestMax(maximumAbove(MediaWiki.Parser.*.tavg,1),10),1,2))'
