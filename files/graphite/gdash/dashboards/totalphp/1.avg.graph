title   "Average Latency Of All PHP Requests -8hours"
linewidth 1
linemode "connected"
hide_legend "false"
from "-8hours"

field :tavg, :color => "blue",
               :alias => "none",
               :data  => 'cactiStyle(alias(MediaWiki.-total.tavg, "Avg PHP Request Latency (ms)"))'

field :tp50, :color => "green",
               :alias => "none",
               :data  => 'cactiStyle(alias(MediaWiki.-total.tp50, "50% PHP Request Latency (ms)"))'
