title   "edit submits/hour Top 10 Wikis After EN.WP -1week"
linewidth 1
linemode "connected"
hide_legend "false"
from "-1 week"

field :a, :alias => "none",
               :data  => 'cactiStyle(substr(highestMax(summarize(exclude(MediaWiki.reqstatsedits.*.submits,"MediaWiki.reqstatsedits.en_wikiped"),"1hour"),10),2,3))'
