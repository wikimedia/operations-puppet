#!/bin/bash

CMD=$1
shift
DBLIST=`</home/wikipedia/common/all.dblist`

for x in $DBLIST; do
	echo -----------------------------------------------------------------
	echo $x 
	echo -----------------------------------------------------------------
	php -ddisplay_errors=On /usr/local/apache/common/multiversion/MWScript.php $CMD --wiki=$x "$@" | sed -u "s/^/$x:  /"
done
