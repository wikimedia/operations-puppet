#!/bin/bash

cd `dirname $0`
BINDIR=/usr/local/bin

$BINDIR/set-group-write

if [ -n "$1" ]; then
        MESSAGE=" '"$@"'"
else
        MESSAGE=
fi


ddsh -F30 -cM -g mediawiki-installation /usr/bin/sync-common

$BINDIR/dologmsg "!log $USER ran sync-common-all $MESSAGE"
$BINDIR/deploy2graphite sync-common-all
