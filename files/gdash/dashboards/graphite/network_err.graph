title   "udp errors/drops, all interfaces"
vtitle  "packets/s"

field :rx_err, :color => "green",
               :alias => "rx errors",
               :data  => "sumSeries(servers.graphite1001.udp.InErrors, servers.graphite1001.udp.NoPorts, servers.graphite1001.udp.RcvbufErrors)"

field :rx_dgram, :color => "blue",
               :alias => "rx datagrams",
               :data  => "servers.graphite1001.udp.InDatagrams"
