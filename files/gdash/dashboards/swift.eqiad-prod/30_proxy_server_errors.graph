title   "proxy server errors/s"

field :proxy_object_errors_get,
    :alias => "object GET 5xx errors",
    :data  => "sumSeries(swift.eqiad-prod.*.proxy-server.object.GET.5*.timing.rate)"

field :proxy_object_errors_delete,
    :alias => "object DELETE 5xx errors",
    :data  => "sumSeries(swift.eqiad-prod.*.proxy-server.object.DELETE.5*.timing.rate)"

field :proxy_object_errors_head,
    :alias => "object HEAD 5xx errors",
    :data  => "sumSeries(swift.eqiad-prod.*.proxy-server.object.HEAD.5*.timing.rate)"

field :proxy_object_errors_post,
    :alias => "object POST 5xx errors",
    :data  => "sumSeries(swift.eqiad-prod.*.proxy-server.object.POST.5*.timing.rate)"

field :proxy_object_errors_put,
    :alias => "object PUT 5xx errors",
    :data  => "sumSeries(swift.eqiad-prod.*.proxy-server.object.PUT.5*.timing.rate)"

field :proxy_errors,
    :alias => "proxy errors count",
    :data  => "sumSeries(swift.eqiad-prod.*.proxy-server.errors.count)"
