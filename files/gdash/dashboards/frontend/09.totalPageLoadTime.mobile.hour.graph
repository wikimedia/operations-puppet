title 'navigationStart to loadEventEnd on mobile sites, last hour'
linewidth 1
linemode 'connected'
hide_legend 'false'
from '-1hour'
vtitle 'milliseconds'

field :q2, :color => 'blue',
           :alias => 'mean',
           :data  => 'frontend.navtiming.totalPageLoadTime.mobile.overall.mean'

field :q3, :color => 'red',
           :alias => '99th percentile',
           :data  => 'frontend.navtiming.totalPageLoadTime.mobile.overall.p99'
