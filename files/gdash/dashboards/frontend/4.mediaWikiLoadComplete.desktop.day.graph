title 'mediaWikiLoadStart to document.onload on desktop sites, last day'
linewidth 1
linemode 'connected'
hide_legend 'false'
from '-1day'
vtitle 'milliseconds'

field :q2, :color => 'blue',
           :alias => 'Median',
           :data  => 'frontend.navtiming.mediaWikiLoadComplete.desktop.overall.median'

field :q3, :color => 'red',
           :alias => '75th percentile',
           :data  => 'frontend.navtiming.mediaWikiLoadComplete.desktop.overall.75percentile'
