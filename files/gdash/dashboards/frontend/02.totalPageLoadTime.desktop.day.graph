title 'navigationStart to loadEventEnd on desktop sites, last day'
linewidth 1
linemode 'connected'
hide_legend 'false'
from '-1day'
vtitle 'milliseconds'

field :q2, :color => 'blue',
           :alias => 'mean',
           :data  => 'frontend.navtiming.totalPageLoadTime.desktop.overall.mean'

field :q3, :color => 'red',
           :alias => '99th percentile',
           :data  => 'frontend.navtiming.totalPageLoadTime.desktop.overall.99percentile'
