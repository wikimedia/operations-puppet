title 'mediaWikiLoadStart to document.onload on desktop sites, last month'
linewidth 1
linemode 'connected'
hide_legend 'false'
from '-1month'
vtitle 'milliseconds'

field :q2, :color => 'blue',
           :alias => 'mean',
           :data  => 'frontend.navtiming.mediaWikiLoadComplete.desktop.overall.mean'

field :q3, :color => 'red',
           :alias => '99th percentile',
           :data  => 'frontend.navtiming.mediaWikiLoadComplete.desktop.overall.99percentile'
