title 'navigationStart to loadEventEnd on mobile sites, last day'
linewidth 1
linemode 'connected'
hide_legend 'false'
from '-1day'
vtitle 'milliseconds'

field :q2, :color => 'blue',
           :alias => 'median',
           :data  => 'frontend.navtiming.totalPageLoadTime.mobile.overall.median'

field :q3, :color => 'red',
           :alias => '99th percentile',
           :data  => 'frontend.navtiming.totalPageLoadTime.mobile.overall.p99'
