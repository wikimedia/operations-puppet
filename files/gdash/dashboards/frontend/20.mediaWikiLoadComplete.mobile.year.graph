title 'mediaWikiLoadStart to document.onload on mobile sites, last year'
linewidth 1
linemode 'connected'
hide_legend 'false'
from '-1year'
vtitle 'milliseconds'

field :q2, :color => 'blue',
           :alias => 'median',
           :data  => 'frontend.navtiming.mediaWikiLoadComplete.mobile.overall.median'

field :q3, :color => 'red',
           :alias => '99th percentile',
           :data  => 'frontend.navtiming.mediaWikiLoadComplete.mobile.overall.p99'
