title 'VisualEditor activation time, one-minute sliding window, last day'
linewidth 1
linemode 'connected'
hide_legend 'false'
from '-1day'
vtitle 'milliseconds'

field :q2, :color => 'blue',
           :alias => 'mean',
           :data  => 've.mwtiming.performance.system.activation.mean'

field :q3, :color => 'red',
           :alias => '99th percentile',
           :data  => 've.mwtiming.performance.system.activation.p99'
