title 'VisualEditor save completion time, one-minute sliding window, last year'
linewidth 1
linemode 'connected'
hide_legend 'false'
from '-1year'
vtitle 'milliseconds'

field :q2, :color => 'blue',
           :alias => 'mean',
           :data  => 've.mwtiming.performance.user.saveComplete.mean'

field :q3, :color => 'red',
           :alias => '99th percentile',
           :data  => 've.mwtiming.performance.user.saveComplete.p99'
