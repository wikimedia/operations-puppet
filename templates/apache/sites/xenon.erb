# Apache configuration for xenon SVG hosting.
# This file is managed by Puppet.
<VirtualHost *:80>
    DocumentRoot /srv/xenon/svgs

    <Directory />
        AllowOverride None
    </Directory>

    <Directory /srv/xenon/svgs>
        Options Indexes
        AllowOverride None
        Order allow,deny
        allow from all
    </Directory>
</VirtualHost>
