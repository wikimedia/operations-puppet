# Apache configuration for gdash.wikimedia.org
# This file is managed by Puppet.
<VirtualHost *:80>
    ServerName gdash.wikimedia.org

    <Location />
        SetHandler uwsgi-handler
        uWSGIForceScriptName /
        uWSGImodifier1 7
        uWSGIsocket /var/run/gdash/gdash.sock
    </Location>
</VirtualHost>
