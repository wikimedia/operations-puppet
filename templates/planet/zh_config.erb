# Global configuration

[Planet]

name            = zh.planet.wikimedia
link            = http://zh.planet.<%= planet_domain_name %>/
owner_name      = planet
owner_email     = planet@wikimedia.org
output_theme    = /usr/share/planet-venus/theme/wikimedia/zh
cache_directory = /var/cache/planet/zh/
output_dir      = /var/www/planet/zh/
feed_timeout    = 20
items_per_page  = 60
log_level       = DEBUG

