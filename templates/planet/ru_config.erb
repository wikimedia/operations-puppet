# Global configuration

[Planet]

name            = ru.planet.wikimedia
link            = http://planet.<%= planet_domain_name %>/ru/
owner_name      = planet
owner_email     = planet@wikimedia.org
output_theme    = /usr/share/planet-venus/theme/wikimedia
cache_directory = /var/cache/planet/ru/
output_dir      = /var/www/planet/ru/
feed_timeout    = 20
items_per_page  = 60
log_level       = DEBUG

