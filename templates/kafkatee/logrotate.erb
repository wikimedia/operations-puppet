# Note:  This file is managed by Puppet.
#
# # Rotate anything in ${webrequest_log_directory}/*.log daily
<%= @webrequest_log_directory %>/*.log {
daily
olddir <%= @webrequest_archive_directory %>
notifempty
nocreate
maxage 180
rotate 1000
dateext
compress
postrotate
service kafkatee reload
endscript
}
