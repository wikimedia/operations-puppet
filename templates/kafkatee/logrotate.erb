# Note:  This file is managed by Puppet.
#
<%= @webrequest_log_directory %>/*.log <%= @webrequest_log_directory %>/*.json <%= @webrequest_log_directory %>/*.tsv {
  daily
  rotate 62
  maxage 62
  olddir <%= @webrequest_log_directory %>/archive
  missingok
  notifempty
  nocreate
  dateext
  compress
  postrotate
    service kafkatee reload
  endscript
}
