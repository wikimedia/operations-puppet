[database]
	username = <%= scope.lookupvar('gerrit::instance::dbuser') %>
	password = <%= scope.lookupvar('gerrit::gerrit_config::gerrit_db_pass') %>

[ldap]
	username = <%= scope.lookupvar('gerrit::gerrit_config::gerrit_ldap_proxyagent') %>
	password = <%= scope.lookupvar('gerrit::gerrit_config::gerrit_ldap_proxyagent_pass') %>

[auth]
	registerEmailPrivateKey = <%= scope.lookupvar('gerrit::gerrit_config::gerrit_email_key') %>
